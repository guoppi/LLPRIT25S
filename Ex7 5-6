using System;
using System.Linq;
using System.Text;

namespace Ex6
{
    class Program
    {
        static void Main(string[] args)
        {
            // Teht채v채 5
            
            int luku;
            do
            {
                luku = int.Parse(Console.ReadLine());

                if (luku >= 0)
                {
                    int nelio = luku * luku;
                    Console.WriteLine(nelio);
                }
            }
            while (luku >= 0);
            

            // Teht채v채 6

            luku = 0;   // alustetaan muuttujan "luku"
            int pienin = int.MaxValue;
            int suurin = int.MinValue;

            do
            {
                luku = int.Parse(Console.ReadLine());

                if (luku >= 0)
                {
                    if (luku < pienin)
                    {
                        pienin = luku;
                    }
                    if (luku > suurin)
                    {
                        suurin = luku;
                    }
                }
            }
            while (luku >= 0);
            Console.WriteLine(pienin);
            Console.WriteLine(suurin);
        }
    }
}
